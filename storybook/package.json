{
  "name": "storybook",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "webpack -p --config webpack.config.js --mode production",
    "build-storybook": "build-storybook -o build",
    "watch": "webpack -p --config webpack.config.js --mode development --watch",
    "storybook": "start-storybook -s ../build -p 6006",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:css": "stylelint --ignore-path storybook/.eslintignore \"storybook/**/*.css\"",
    "lint:css:fix": "yarn run lint:css --fix",
    "lint:js": "eslint --ignore-path ./.eslintignore ./ --ext js",
    "lint:js:fix": "yarn run lint:js --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-rewrite": "./hooks/post-rewrite.sh ${HUSKY_GIT_PARAMS}"
    }
  },
  "lint-staged": {
    "*.{js,json,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@storybook/addon-a11y": "^5.2.3",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-knobs": "^5.2.3",
    "@storybook/addon-notes": "^5.2.3",
    "@storybook/addon-viewport": "^5.2.3",
    "@storybook/addons": "^5.2.3",
    "@storybook/client-api": "^5.2.3",
    "@storybook/core-events": "^5.2.3",
    "@storybook/html": "^5.2.3",
    "@storybook/theming": "^5.2.3",
    "@types/node": "^11.13.22",
    "@webcomponents/webcomponentsjs": "^2.3.0",
    "autoprefixer": "^9.6.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "^3.2.1",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.9.2",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.6.0",
    "postcss-url": "^8.0.0",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^7.3.1",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-sass-guidelines": "^5.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.1.0",
    "stylelint-scss": "^3.11.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "tailwindcss": "^1.1.2",
    "twig": "^1.13.2",
    "twig-drupal-filters": "^2.0.0",
    "twig-loader": "git+https://github.com/AmazeeLabs/twig-loader.git",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-fix-style-only-entries": "^0.3.1"
  },
  "resolutions": {
    "browserslist": "4.6.2",
    "caniuse-lite": "1.0.30000974"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2"
  }
}
